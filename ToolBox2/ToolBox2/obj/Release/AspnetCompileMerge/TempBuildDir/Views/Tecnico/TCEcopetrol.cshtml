
@{
    ViewBag.Title = "TCEcopetrol";
    Layout = "~/Views/Shared/TCLayout.cshtml";
}

<div class="container fondotext">
    <div class="eco">
        <h2>Ecopetrol</h2>
        <br />
    </div>
    <div>
        <h3>Personal</h3>
        <div class="col-md-12">
            <h4>Coordinador</h4>
        </div>
        <div class="col-md-12">
            <table id="Coord"></table>
            <script type="text/javascript">
                $(document).ready(function () {
                    ListaCoord();
                })
            </script>
        </div>
        <br />
        <div class="col-md-12">
            <h4>Tecnicos</h4>
        </div>
